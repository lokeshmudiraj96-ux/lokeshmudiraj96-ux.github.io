{
  "version": 2,
  "name": "quickbite-demo-backend",
  "builds": [
    {
      "src": "quickbite-backend/demo-setup.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "quickbite-backend/demo-setup.js"
    },
    {
      "src": "/(.*)",
      "dest": "quickbite-backend/demo-setup.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}