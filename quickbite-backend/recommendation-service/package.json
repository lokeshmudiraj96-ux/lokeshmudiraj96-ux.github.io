{
  "name": "quickbite-recommendation-service",
  "version": "1.0.0",
  "description": "AI-Powered Recommendation Engine for QuickBite - Machine learning system for personalized food recommendations, menu optimization, and user experience enhancement",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "train-models": "node src/scripts/train-models.js",
    "update-recommendations": "node src/scripts/update-recommendations.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t quickbite-recommendation-service .",
    "docker:run": "docker run -p 3008:3008 quickbite-recommendation-service"
  },
  "keywords": [
    "recommendation-engine",
    "machine-learning",
    "collaborative-filtering",
    "content-based-filtering",
    "food-recommendations",
    "personalization",
    "ai",
    "nodejs",
    "express",
    "tensorflow",
    "scikit-learn"
  ],
  "author": "QuickBite Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "ml-matrix": "^6.10.7",
    "ml-kmeans": "^6.0.0",
    "ml-knn": "^3.0.0",
    "natural": "^6.8.0",
    "compromise": "^14.11.0",
    "sentiment": "^5.0.2",
    "stopword": "^2.0.8",
    
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "node-cron": "^3.0.3",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    
    "axios": "^1.6.2",
    "node-fetch": "^3.3.2",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0",
    "fast-csv": "^4.3.6",
    
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "express-async-errors": "^3.1.1",
    
    "mathjs": "^12.2.1",
    "d3": "^7.8.5",
    "simple-statistics": "^7.8.3",
    "regression": "^2.0.1",
    "clustering-js": "^1.0.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "@types/jest": "^29.5.8",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lokeshmudiraj96-ux/lokeshmudiraj96-ux.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/lokeshmudiraj96-ux/lokeshmudiraj96-ux.github.io/issues"
  },
  "homepage": "https://github.com/lokeshmudiraj96-ux/lokeshmudiraj96-ux.github.io#readme",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/config/*.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/src/**/*.test.js"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}